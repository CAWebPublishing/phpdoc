{% set colorscheme = 'santa barbara' %}

{# phpDocumentor Settings are not working as of v3.0.0 #}
{% set stable_version = '1.5.3' %}
{% set github_ref = 'https://github.com/' %}
{% set github_user = 'CA-CODE-Works' %}
{% set github_branch = 'dev' %}

{% set gitub_href_link = github_ref ~ github_user ~ '/CAWeb/blob/' ~ github_branch ~ '/' %}

<!DOCTYPE html>
<html class="no-js" lang="en">
    <head>
        {% block head %}
        {% endblock head %}
        
        {{ include('/vendor/caweb/phpdoc/inc/head.html') }}
    </head>
    <body class="primary non_divi_builder">
        {{ include('/vendor/caweb/phpdoc/inc/header.html') }}

        <div id="page-container">
            <div id="et-main-area">
                <div id="main-content" class="main-content" tabindex="-1">
                    <main class="main-primary">
                        {% block introduction %}
                        {% endblock introduction %}
                        
                        {% block table_of_contents %}
                        {% endblock table_of_contents %}

                        {% block body %}
                        {% endblock body %}

                    </main>
                </div>
            </div>
        </div>

        <footer id="footer" class="global-footer hidden-print">
            {{ include('/vendor/caweb/phpdoc/inc/footer.html') }}
            <!-- Load template core -->
            <script src="{{ path('/')  }}js/caweb.core.js"></script>
        </footer>
    </body>
</html>
