{% extends "vendor/caweb/phpdoc/layout.html.twig" %}

{% block head %}
<title>CAWeb Examples</title>

{% endblock head %}

{% block introduction %}
<div class="section">
<h1>Examples</h1>
    <p>
        The CAWeb Theme is a child theme of <a target="_blank" href="https://www.elegantthemes.com/gallery/divi/">Divi</a>, and fully integrates the <a target="_blank" href="https://template.webtools.ca.gov/">California State Template</a>.
        The following pages include various examples of the options available. 
        </p>
    
</div>
{% endblock introduction %}

{% block table_of_contents %}
{% endblock table_of_contents %}

{% block body %}
<div class="section">
    <h2>Icon Font Library</h2>
    <a href="{{ path('/') }}examples/icon-library.html">Icon Font Library</a>
    <h2 class="mt-3">ColorSchemes</h2>
    <ol class="list-style-none pl-2 mt-0">
        {% for color,data in colorschemes %}
        <li><a href="{{ path('/') }}examples/{{ color|replace({' ' : '-'})|lower }}.html">{{ color }}</a></li>
        {% endfor %}
    </ol>
</div>
{% endblock body %}