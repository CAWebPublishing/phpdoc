<?xml version="1.0" encoding="utf-8"?>
<template>
    <name>caweb</name>
    <author>Danny Guzman</author>
    <email>guzmanjd86@gmail.com</email>
    <description>phpDocumentor Template Config File for the CAWeb WordPress Theme</description>
    <version>1.0.0</version>
    <parameters>
        <parameter key="twig-debug">true</parameter>
    </parameters>
    <transformations>
        <transformation writer="FileIo" query="copy" source="css/" artifact="css/" />
        <transformation writer="FileIo" query="copy" source="imgs/caweb_logo.ico" artifact="favicon.ico" />
        <transformation writer="FileIo" query="copy" source="imgs/caweb-logo.png" artifact="imgs/branding.png" />
